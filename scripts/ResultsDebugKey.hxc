import flixel.FlxG;
import funkin.modding.module.Module;

class ResultsDebugKey extends Module
{
  public function new()
  {
    super('ResultsDebugKey');
  }

  override function onUpdate(event:UpdateScriptEvent)
  {
    super.onUpdate(event);

    if (FlxG.keys.justPressed.F9) FlxG.switchState(() -> new funkin.ui.debug.results.ResultsDebugSubState());
  }
}
